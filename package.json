    "react-scripts": "1.1.5",
    "reactjs-calendar-heatmap": "^0.0.10",
    "recharts": "^1.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --no-watchman",
