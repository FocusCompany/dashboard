    "babel-loader": "^8.0.4",
    "husky": "^1.1.3",
    "prettier": "^1.14.3",
    "react-test-renderer": "^16.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
